{
  "name": "Tumble",
  "version": "0.0.1",
  "description": "Tumblr client for desktop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . Tumble --out=publish --asar --overwrite --ignore=\"(main|render)/\" --ignore=\"\\.gitignore\" --ignore=\"create_consumer.js\" --platform=darwin --ia=x64 --icon=icon/Tumble.icns",
    "build-win": "electron-packager . Tumble --out=publish --asar --overwrite --ignore=\"(main|render)/\" --ignore=\"\\.gitignore\" --ignore=\"create_consumer.js\" --platform=win32 --ia=x64 --icon=icon/Tumble.ico",
    "build-all": "webpack && npm run build-mac && npm run build-win",
    "package-mac": "npm run build-mac && zip -rv publish/Tumble-darwin-x64.zip publish/Tumble-darwin-x64/",
    "package-win": "npm run build-win && 7z a publish/Tumble-win32-x64.zip publish/Tumble-win32-x64/",
    "package-all": "npm run package-mac && npm run package-win",
    "clearn": "rm -rf publish",
    "watch": "webpack --watch",
    "consumer": "node create_consumer.js",
    "test": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eivy/Tumble"
  },
  "author": "Eivy",
  "license": "ISC",
  "dependencies": {
    "about-window": "^1.9.1",
    "electron-store": "^1.3.0",
    "oauth-1.0a": "^2.2.4",
    "request": "^2.83.0",
    "tumblr.js": "^1.1.1",
    "vue": "^2.5.13",
    "vue-awesome": "^2.3.4",
    "vue-loader": "^13.7.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vue-waterfall": "^1.0.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "^1.8.1",
    "electron-packager": "^10.1.2",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "prompt": "^1.0.0",
    "sass-loader": "^6.0.6",
    "source-map": "^0.6.1",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-obfuscator": "^0.13.0"
  }
}
