{
  "name": "Tumble",
  "version": "0.9.0",
  "description": "Tumblr client for desktop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "webpack && electron-packager . Tumble --overwrite --ignore='(main|render|node_modules)/' --platform=darwin --icon=icon/Tumble.icns",
    "build-win": "webpack && electron-packager . Tumble --overwrite --ignore='(main|render|node_modules)/' --platform=win32",
    "watch": "webpack --watch",
    "consumer": "node create_consumer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eivy/Tumble"
  },
  "author": "Eivy",
  "license": "ISC",
  "dependencies": {
    "electron-store": "^1.3.0",
    "oauth-1.0a": "^2.2.3",
    "request": "^2.83.0",
    "tumblr.js": "^1.1.1",
    "vue": "^2.5.13",
    "vue-awesome": "^2.3.4",
    "vue-loader": "^13.6.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vue-waterfall": "^1.0.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "electron": "^1.8.1",
    "electron-packager": "^10.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "prompt": "^1.0.0",
    "sass-loader": "^6.0.6",
    "source-map": "^0.6.1",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-obfuscator": "^0.13.0"
  }
}
